<html>
<head>
  <link rel="icon" href="/static/hyottoko.png" sizes="72x72" type="image/png"/>
  <title>{{ title }}</title>
  <script src="https://cdn.jsdelivr.net/npm/vue@2.6.11"></script>
  <link rel="stylesheet" href="/static/widgets/{{ page }}.css"/>
  <meta charset="utf-8"/>
</head>
<body>
<div id="outer_main">
  <div id="app"></div>
  <script type="module">
  import Page from "/static/widgets/{{ page }}.js"
  import WsClient from "/static/WsClient.js"

  const ws = new WsClient(
    {{ wsUrl|json_encode|raw }},
    {{ widgetToken|json_encode|raw }},
  )
  new Vue({ el: '#app', render: (h) => h(Page, { props: { ws: ws } }) })
  </script>
</div>
</body>
</html>
